{
  "hosting": {
    "public": ".output/public",
    "cleanUrls": true,
    "headers": [
      {
        "source": "/videos/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, s-maxage=31536000, immutable"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "server",
          "region": "us-central1"
        }
      }
    ]
  },
  "functions": [
    {
      "source": ".output/server",
      "codebase": "portfolio",
      "runtime": "nodejs20"
    }
  ]
}

